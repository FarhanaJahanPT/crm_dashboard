<?xml version="1.0" encoding="utf-8" ?>
<templates>
   <t t-name="CustomDashBoard">
       <div class="oh_dashboards" style="margin-top: 20px;">
           <div class="container-fluid o_pj_dashboard">
           </div>
       </div>
   </t>
       <t t-name="DashboardProject">
    <section class="dashboard_main_section" id="main_section_login">
        <div style="height:50px">
         </div>
        <div class="row">
            <div class="col-sm-12 mb-4">
                <div class="row">
                    <div class="col-12 col-sm-12 col-md-8">
                        <h2 class="section-header">CRM Dashboard</h2>
                    </div>
                    <div class="col-12 col-sm-12 col-md-4">
                        <form class="form-group">
                            <select id="income_expense_values" class="form-control">
                            <option id="this_year" value="this_year">This Year</option>
                            <option id="this_quarter" value="this_quarter">This Quarter</option>
                            <option id="this_month" value="this_month" selected="">This Month</option>
                            <option id="this_week" value="this_week">This Week</option>
                            </select>
                        </form>
                    </div>
                </div>
                <hr/>
            </div>
        </div>
    </section>
</t>
<t t-name="Managercrm">
<!--<div class="scrollbar" id="style-1">-->
<!--			<div class="force-overflow">-->

<!--            </div>-->
    <div style="width:100px">
         </div>
    <section class="dashboard_main_section" id="main_section_manager">
        <div style="height:50px">
         </div>
         <div class="row">

        <div class="crm_dash_cards row">
            <div class="col-sm-12 col-md-3">
                <style>
                    #my_lead_dashboard{
                     width: 300px;
                      border: 2px solid black;
                      padding: 10px;
                      margin: 10px;}
                </style>
                <div class="dashboard-card card-shadow my_lead" id="my_lead_dashboard">
                    <div class="dashboard-card__icon-container bg-mauve-light d-flex justify-content-center align-items-center">
                    <i class="fa fa-line-chart text-mauve"/>
                    </div>
                    <div class="dashboard-card__details">
                    <h3>
                        <span>
                        <div id="lead_dashboard"/>
                        <div id="leads_this_year"/>
                        <div id="leads_this_quarter"/>
                        <div id="leads_this_month"/>
                        <div id="leads_this_week"/>
                        </span>
                    </h3>
                <h4>My Leads</h4>
                    </div>
                </div>
            </div>
            <div class="col-sm-12 col-md-3">
                <style>
                    #my_opportunity_dashboard{
                     width: 300px;
                      border: 2px solid black;
                      padding: 10px;
                      margin: 10px;}
                </style>
                <div class="dashboard-card card-shadow opportunity" id="my_opportunity_dashboard">
                    <div class="dashboard-card__icon-container bg-mauve-light d-flex justify-content-center align-items-center">
                        <i class="fa fa-trophy text-mauve"/>
                    </div>
                    <div class="dashboard-card__details" id="dashboard_card_opportunity">
                    <h3>
                        <span>
                        <div id="opportunity_dashboard"/>
                        <div id="opp_this_year"/>
                        <div id="opp_this_quarter"/>
                        <div id="opp_this_month"/>
                        <div id="opp_this_week"/>
                        </span>
                    </h3>
                    <h4>My Opportunities</h4>
                </div>
            </div>
            </div>
            <div class="col-sm-12 col-md-3">
                <style>
                    #my_exp_revenue_dashboard{
                     width: 300px;
                      border: 2px solid black;
                      padding: 10px;
                      margin: 10px;}
                </style>
                <div class="dashboard-card card-shadow exp_revenue" id="my_exp_revenue_dashboard">
                    <div class="dashboard-card__icon-container bg-mauve-light d-flex justify-content-center align-items-center">
                        <i class="fa fa-usd text-mauve"/>
                    </div>
                    <div class="dashboard-card__details">
                    <h3>
                        <span>
                        <div id="expected_revenue_dashboard"/>
                        <div id="exp_rev_this_year"/>
                        <div id="exp_rev_this_quarter"/>
                        <div id="exp_rev_this_month"/>
                        <div id="exp_rev_this_week"/>
                        </span>
                    </h3>
                    <h4>Expected Revenue</h4>
                </div>
                </div>
            </div>
            <div class="col-sm-12 col-md-3">
                <style>
                    #my_revenue_dashboard{
                     width: 300px;
                      border: 2px solid black;
                      padding: 10px;
                      margin: 10px;}
                </style>
                <div class="dashboard-card card-shadow revenue_card revenue_card" id="my_revenue_dashboard">
                    <div class="dashboard-card__icon-container bg-mauve-light d-flex justify-content-center align-items-center">
                        <i class="fa fa-usd text-mauve"/>
                    </div>
                    <div class="dashboard-card__details">
                    <h3>
                        <span>
                        <div id="total_revenue_dashboard"/>
                        <div id="rev_this_year"/>
                        <div id="rev_this_quarter"/>
                        <div id="rev_this_month"/>
                        <div id="rev_this_week"/>
                        </span>
                    </h3>
                    <h4>Revenue</h4>
                </div>
                </div>
            </div>
        <div class="col-sm-12 col-md-3">
            <style>
                    #my_win_ratio_dashboard{
                     width: 300px;
                      border: 2px solid black;
                      padding: 10px;
                      margin: 10px;}
                </style>
            <div class="dashboard-card card-shadow win_ratio" id="my_win_ratio_dashboard">
                <div class="dashboard-card__icon-container bg-mauve-light d-flex justify-content-center align-items-center">
                    <i class="fa fa-percent text-mauve"/>
                </div>
                <div class="dashboard-card__details">
                <h3>
                    <span>
                    <div id="win_ratio_dashboard"/>
                    <div id="ratio_this_year"/>
                    <div id="ratio_this_quarter"/>
                    <div id="ratio_this_month"/>
                    <div id="ratio_this_week"/>
                    </span>
                </h3>
                <h4>Win Ratio</h4>
            </div>
            </div>
        </div>
</div>
         </div>
        <div style="height:50px">
         </div>

        <div class="row">

             <div style="width:100px">
         </div>

    <div class="col-sm-12 col-md-4">
        <div class="total_leads_manager">
            <h2>Total Leads</h2>
            <hr/>
            <div class="graph_canvas">
                <canvas class="total_lead_year" width="200" height="120" id="activity_year"/>
                 <canvas class="total_lead_week" width="200" height="120" id="activity_week"/>
                 <canvas class="total_lead_month" width="200" height="120" id="activity_month"/>
                 <canvas class="total_lead_quarter" width="200" height="120" id="activity_quarter"/>
            </div>
        </div>
    </div>



            <div style="width:400px">
         </div>

    <div class="col-sm-12 col-md-4">
        <div class="total_opportunity_manager">
            <h2>Total Opportunity</h2>
            <hr/>
            <div class="graph_canvas">
                <canvas class="total_opportunity_year" width="200" height="120" id="opportunity_year"/>
                <canvas class="total_opportunity_month" width="200" height="120" id="opportunity_month"/>
                <canvas class="total_opportunity_week" width="200" height="120" id="opportunity_week"/>
                <canvas class="total_opportunity_quarter" width="200" height="120" id="opportunity_quarter"/>
            </div>
        </div>
    </div>
        </div>
        <div style="height:50px">
         </div>
        <div class="row">
            <div style="width:100px">
         </div>
        <div class="col-sm-12 col-md-4">
        <div class="total_expected_revenue">
            <h2>Total Expected Revenue</h2>
            <hr/>
            <div class="doughnut">
                <canvas class="total_exp_revenue_year" width="200" height="120" id="total_revenue_year"/>
                <canvas class="total_exp_revenue_month" width="200" height="120" id="total_revenue_month"/>
                <canvas class="total_exp_revenue_week" width="200" height="120" id="total_revenue_week"/>
                <canvas class="total_exp_revenue_quarter" width="200" height="120" id="total_revenue_quarter"/>
            </div>
        </div>
    </div>
            <div style="width:400px">
         </div>
        <div class="col-sm-12 col-md-4">
        <div class="total_revenue">
            <h2>Total Revenue</h2>
            <hr/>
            <div class="doughnut">
                <canvas class="total_com_revenue_year" width="200" height="120" id="total_comp_revenue_year"/>
                <canvas class="total_com_revenue_month" width="200" height="120" id="total_comp_revenue_month"/>
                <canvas class="total_com_revenue_week" width="200" height="120" id="total_comp_revenue_week"/>
                <canvas class="total_com_revenue_quarter" width="200" height="120" id="total_comp_revenue_quarter"/>
            </div>
        </div>
    </div>
        </div>
                <div style="height:50px">
         </div>

        <div class="row">
                        <div style="width:600px">
         </div>

        <div class="col-sm-12 col-md-4">
        <div class="total_win_ratio">
            <h2>Win Ratio</h2>
            <hr/>
            <div class="pie">
                <canvas class="win_ratio_year" width="200" height="120" id="win_ratio_expense_year"/>
                <canvas class="win_ratio_month" width="200" height="120" id="win_ratio_expense_month"/>
                <canvas class="win_ratio_week" width="200" height="120" id="win_ratio_expense_week"/>
                <canvas class="win_ratio_quarter" width="200" height="120" id="win_ratio_expense_quarter"/>
            </div>
        </div>
    </div>

        </div>

</section>
<!--    </div>-->
</t>
</templates>